{
  "name": "node-blockly",
  "version": "1.2.8",
  "description": "Blockly for Node.js and Browser as CommonJS module",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/",
    "/*.js",
    "!/gulpfile.js",
    "index.d.ts"
  ],
  "keywords": [
    "blockly",
    "commonjs"
  ],
  "dependencies": {
    "jsdom": "^16.5.0",
    "xmlshim": "^0.2.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-beautify": "^3.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^6.1.4"
  },
  "scripts": {
    "blockly:upgrade": "git submodule init && git submodule update --recursive --remote",
    "prepublish": "rm -rf lib && mkdir lib && gulp build",
